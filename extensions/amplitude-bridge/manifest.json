{
  "manifest_version": 3,
  "name": "DagNet Amplitude Bridge",
  "version": "0.1.2",
  "description": "Allows DagNet to create funnel charts in your Amplitude workspace.",
  "key": "",
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://app.amplitude.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:*/*",
      "https://localhost:*/*",
      "https://*.vercel.app/*"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
