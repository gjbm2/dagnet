  "version": "0.94.2-beta",
  "type": "module",
  "scripts": {
    "dev": "npm run generate-docs && vite",
    "generate-docs": "node scripts/generate-docs-index.js",
    "prebuild": "npm run generate-docs",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:all": "vitest run tests/",
    "test:unit": "vitest run tests/unit/ tests/smoke.test.ts",
    "test:integration": "vitest run tests/integration/",
    "test:smoke": "vitest run tests/smoke.test.ts",
    "test:watch": "vitest --watch tests/",
    "test:watch:unit": "vitest --watch tests/unit/",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage tests/",
    "ci:check": "npm ci && npx tsc --noEmit",
    "ci:lint": "npx tsc --noEmit",
    "ci:test": "npm run test:p0 && npm run test:run"
