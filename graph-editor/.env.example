# Git Repository Configuration
VITE_GIT_REPO_OWNER=your-github-username
VITE_GIT_REPO_NAME=your-repo-name
VITE_GIT_REPO_BRANCH=main
VITE_GIT_GRAPHS_PATH=graphs
VITE_GIT_PARAMS_PATH=params
VITE_GITHUB_API_BASE=https://api.github.com
VITE_GITHUB_TOKEN=your_github_token_here
VITE_DEV_MODE=true
VITE_DEBUG_GIT_OPERATIONS=false

# Apps Script Configuration
VITE_APPS_SCRIPT_URL=https://script.google.com/macros/s/DEPLOYMENT_ID/exec

# Local Development Server Ports
VITE_PORT=5173
VITE_PYTHON_API_PORT=9000
VITE_PYTHON_API_URL=http://localhost:9000

# Python Mock Mode (set to 'true' to develop frontend without Python backend)
VITE_USE_MOCK_COMPUTE=false
